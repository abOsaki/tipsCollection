/* Chromeのuser agentをリセット(table)*/

table {
    border-spacing: 0;
    border-color: transparent;
    border-collapse: collapse;
}

.table>thead>tr>th {
    border-bottom-width: 1px;
}

tbody {
    display: table-row-group;
    vertical-align: middle;
    border-color: transparent;
}

html,
body,
div,
ul,
ol,
li,
p {
    margin: 0px;
    padding: 0px;
    top: 0px;
    left: 0px;
    font-size: 10pt;
}

html,
body {
    background-image: url(../common/images/bg_green.jpg);
    background-position: right top;
    background-repeat: repeat-y;
    background-color: #a2d8fe;
}

#container {
    position: absolute;
    margin: auto;
    top: 0px;
    right: 0px;
    left: 0px;
    width: 100%;
    height: 100%;
}

#header {
    position: relative;
    font-family: "ヒラギノ角ゴ Pro W3", "Hiragino Kaku Gothic Pro", "メイリオ", Meiryo, Osaka, "ＭＳ Ｐゴシック", "MS PGothic", sans-serif;
    font-size: 18px;
    font-weight: bold;
    color: #FFF;
    padding-left: 25px;
    padding-top: 10px;
    width: 600px;
    float: left;
    height: 40px;
}

#header span {
    font-family: "ヒラギノ角ゴ Pro W3", "Hiragino Kaku Gothic Pro", "メイリオ", Meiryo, Osaka, "ＭＳ Ｐゴシック", "MS PGothic", sans-serif;
    font-size: 15px;
    font-weight: bold;
    color: #FFF;
    background-color: #06C;
    margin-left: 15px;
    padding-top: 5px;
    padding-right: 10px;
    padding-bottom: 5px;
    padding-left: 10px;
}

#contents {
    position: relative;
    margin: auto;
    top: 0px;
    right: 0px;
    bottom: 0px;
    left: 0px;
    width: 97%;
}

#contents {
    /*　コンテンツ */
    position: relative;
    height: 96%;
}

#menu_non {
    position: relative;
    height: 10px;
}

.sp {
    width: 295px;
}

#basicInfo {
    position: relative;
    border-top-left-radius: 8px;
    border-top-right-radius: 8px;
    height: 50px;
    background-color: #ffffff;
    box-shadow: 0px 3px 5px rgba(0, 0, 0, 0.4) inset;
    margin: 0px auto;
    background-repeat: no-repeat;
    background-position: 15px;
}

#basicInfo div {
    margin: 0px 0px 0pc 15px;
    display: inline;
    font-size: 16px;
    font-family: "ＭＳ ゴシック", "MS Gothic", "Osaka－等幅", Osaka-mono, monospace;
}

#grade {
    width: 80px;
}

#curriculum {
    width: 100px;
}

#unitSelect {
    width: 100px;
}

#quesTitle {
    width: 220px;
}

#quesAuthor {
    width: 110px;
}

#questionBox {
    font-size: 16px;
    height: 90%;
    border: 4px ridge #46a0e2;
    background-color: #FFFFFF;
    margin: 0 auto;
}


/*普通のボタン*/

#img_hyouji {}

#img_csv {}


/*小さいボタン*/

#img_menu {
    float: right;
    margin-top: 10px;
    margin-right: 6px;
    top: 15px;
}

#img_modoru {
    float: right;
    margin-top: 10px;
    margin-right: 6px;
    top: 15px;
}


/*ログアウト追加*/

#img_logout {
    float: right;
    margin-top: 10px;
    margin-right: 6px;
    top: 15px;
}


/*検索情報*/

#group {
    width: 70px;
}

#school {
    width: 100px;
}

#grade {
    width: 70px;
}

#curriculum {
    width: 70px;
}

#unit {
    width: 100px;
}

#title {
    width: 220px;
}

#author {
    width: 110px;
}

td {
    font-size: 16px;
    font-family: "メイリオ", "Hiragino Kaku Gothic Pro", Meiryo, Osaka, "ＭＳ Ｐゴシック", "MS PGothic", sans-serif;
}

th {
    font-size: 15px;
    /*font-weight: bold;*/
    text-align: center;
    background-color: #badcff;
    color: #0f0f0f;
}

.allTable th {
    font-size: 15px;
    /*font-weight: bold;*/
    text-align: center;
    background-color: #f5d7bc;
    color: #0f0f0f;
    cursor: pointer;
}

tr:nth-child(2n) {
    background: #ededed;
}

hr {
    margin: 8px 0;
    clear: both;
    height: 0;
}

ul#editing li {
    list-style: none;
    display: inline;
    padding-right: 10px;
    padding-left: 10px;
}


/*ボタンスタイル*/

#averageFilter input {
    vertical-align: middle;
}


/*テーブル*/

.tableHeader,
.tableBody {
    padding: 0 -10px;
}

#questionBox {
    position: relative;
}

#questionBox > img {
    position: relative;
    z-index: 1;
}

#questionBox h1 {
    font-size: 18px;
    font-weight: bold;
    color: #fff;
    background-color: #32B24F;
    text-align: center;
    padding: 5px;
}

#table01 {
    padding-top: 10px;
    min-width: 2000px;
    padding-left: 50px;
}

#table01 table {
    /*border-collapse: collapse;*/
    /*    background-color: #8DD6EA;*/
    /*margin:10px auto;*/
}

.tableHeader {
    padding-right: 17px;
}

.tableHeader tr {
    height: 25px;
    font-size: 11pt;
}

.tableBody {
    position: absolute;
    width: 100%;
    top: 115px;
    bottom: 0px;
    overflow-y: scroll;
}

th,
td {
    border: solid 1px #333;
    border-width: 0 1px 1px 0;
    text-align: center;
}

td img {
    margin: 5px auto 0;
}

.tableBody tr:nth-child(2n) {
    background: #ededed;
}


/*選択*/

#img_edit,
#img_search {
    display: block;
    float: right;
    margin-right: 20px;
}

#basicInfo div {
    display: inline-block;
    margin: 15px 10px;
}

#btnImg {
    position: absolute;
    display: inline-block;
    top: -5px;
    left: 900px;
    margin: 0px;
    width: 300px;
}

.hide {
    display: none;
}

.allTable {
    border-collapse: separate;
    border: 1px solid #000;
    border-width: 1px 0 0 1px;
    -webkit-border-top-left-radius: 25px;
    -moz-border-top-left-radius: 25px;
    border-top-left-radius: 25px;
    -webkit-border-top-right-radius: 25px;
    -moz-border-top-right-radius: 25px;
    border-top-right-radius: 25px;
}

.allTable thead tr th:nth-child(1) {
    -webkit-border-top-left-radius: 25px;
    -moz-border-top-left-radius: 25px;
    border-top-left-radius: 25px;
}

.allTable thead tr th:nth-last-child(1) {
    -webkit-border-top-right-radius: 25px;
    -moz-border-top-right-radius: 25px;
    border-top-right-radius: 25px;
}

.allTable thead tr th,
.allTable tbody tr td,
.student thead tr th,
.student tbody tr td {
    width: 100px!important;
    font-size: 14px;
}

.allTable thead tr th:nth-child(1),
.student thead tr th:nth-child(1),
.allTable tbody tr td:nth-child(1),
.student tbody tr td:nth-child(1) {
    width: 300px!important;
    border-right-width: 3px;
}

.allTable tbody tr td:nth-child(1),
.student tbody tr td:nth-child(1) {
    font-weight: bolder;
    /*background-color: #e9e9e9;*/
}

.allTable .warning {
    background-position: center center;
    background-image: url(../common/images/warning2.png);
    background-size: 50% auto;
    background-repeat: no-repeat;
}

.allTable .error {
    background-position: center center;
    background-image: url(../common/images/error2.png);
    background-size: 50% auto;
    background-repeat: no-repeat;
}

.red {
    font-weight: bolder;
    color: #FF3100;
    font-size: 16px!important;
    /*    text-shadow: #333 1px 1px 0px, #333 -1px 1px 0px,
             #333 1px -1px 0px, #333 -1px -1px 0px;*/
}

.orange {
    color: #FF3100;
}

.student {
    border: 1px solid black;
    border-width: 0 0 0 1px;
}

.pointer {
    cursor: pointer;
}

.studentWrap {
    display: none;
}

#navigation_tab {
    text-align: center;
    margin: 20px auto;
}

#navigation_tab div {
    width: 100px;
    margin: 0 20px;
    cursor: pointer;
}

#graphBox {
    display: none;
    padding: 10px 50px;
}

#kakuninBox {}

#outputArea {
    width: 99%;
    height: 90%;
    overflow: auto;
}

#-ccchart-css-hybrid {
    display: none;
}

#tagselectArea .tagSelect,
#sexselectArea .tagSelect {
    user-select: none;
    /* CSS3 */
    -moz-user-select: none;
    /* Firefox */
    -webkit-user-select: none;
    /* Safari、Chromeなど */
    -ms-user-select: none;
    /* IE10かららしい */
    margin: 5px;
    cursor: pointer;
}

.gbtn {
    width: 50px;
    margin: 1px;
    font-size: 12px!important;
    font-weight: bold;
    text-decoration: none;
    display: inline-block;
    text-align: center;
    padding: 3px 5px;
    color: #fff;
    background-color: #ccc;
    border-radius: 5px;
}

.rbtn-on {
    width: 50px;
    margin: 1px;
    font-size: 12px!important;
    font-weight: bold;
    text-decoration: none;
    display: inline-block;
    text-align: center;
    padding: 3px 5px;
    color: #fff;
    background-color: orange;
    border-radius: 5px;
}

.txt {
    vertical-align: middle;
}

#grade,
#class,
#pMonth,
#pTitle {
    vertical-align: middle;
    font-weight: bold;
    border: solid 1px #a0a0a0;
    padding: 3px 3px 0 3px;
}

#graph {
    border: 4px ridge #46a0e2;
}

#averageFilter {
    display: none;
}